local Rayfield = loadstring(game:HttpGet('https://sirius.menu/rayfield'))()

local Window = Rayfield:CreateWindow({
   Name = "ByteFlow",
   Icon = 0,
   LoadingTitle = "The King of all Rayfields",
   LoadingSubtitle = "by real_kj88888",
   ShowText = "ByteFlow",
   Theme = "Ocean",
   ToggleUIKeybind = "K",

   DisableRayfieldPrompts = false,
   DisableBuildWarnings = false,

   ConfigurationSaving = {
      Enabled = true,
      FolderName = nil,
      FileName = "Big Hub"
   },

   Discord = {
      Enabled = false,
      Invite = "noinvitelink",
      RememberJoins = true
   },

   KeySystem = false,
   KeySettings = {
      Title = "Untitled",
      Subtitle = "Key System",
      Note = "No method of obtaining the key is provided",
      FileName = "Key",
      SaveKey = true,
      GrabKeyFromSite = false,
      Key = {"Hello"}
   }
})


local function makeButton(tab, name, url)
   tab:CreateButton({
      Name = name,
      Callback = function()
         loadstring(game:HttpGet(url))()
      end,
   })
end



